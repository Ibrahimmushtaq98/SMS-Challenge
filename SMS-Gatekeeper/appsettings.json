{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "RateLimiterOptions": {
    "MaximumMessagePerAccountPerSecond": 3,
    "MaximumMessagePerBuisnessNumberPerSecond": 3,
    "InactivityTimeoutSecond": 60,
    "CleanupIntervalSecond":  120
  }
}
